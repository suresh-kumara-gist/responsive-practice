<script type="text/javascript">

//$('.error_cls').hide();	

var windowWidth = $(window).width();



$(window).resize(function() {

if(windowWidth != $(window).width()){

location.reload();

return;

}

});

jQuery(document).ready(function($){

$('.error_cls').hide();	

		$("#footerEmailIcons").click(function(){

		centerPopup('#contact-form', top.document);

		loadPopup('#contact-form', top.document);

		return false;

		});

			

		$("#footerEmailIcon").click(function(){

		centerPopup('#contact-form', top.document);

		loadPopup('#contact-form', top.document);

		return false;

		});

	$('#close-popup').click(function(){

		disablePopup('#contact-form');	

	});

		

		$("#formFeedbackId").submit(function() { 

    	

        // Specify the validation rules

			var formFeedbackName = $("#mailFromName").val();

			var companyname = $("#mailCompanyName").val();			

			var formFeedbackSubject = $("#mailSubject").val();

			var formFeedbackEmail = $("#mailFrom").val();

						

			var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;

			$('.error_cls').hide();	

			

			if(formFeedbackName == '')

			{

				$('#error_mailFromName').text('Please Enter Name!');

				$('#error_mailFromName').fadeIn(400).show();

				$("#mailFromName").focus();

				return false;

			}

			

			if( formFeedbackName.indexOf('www.') >= 0){

  				$('#error_mailFromName').text('Please Enter Valid Name!');

				$('#error_mailFromName').fadeIn(400).show();

				$("#mailFromName").focus();

				return false;

			}

			

			if( formFeedbackName.indexOf('http://') >= 0){

			  	$('#error_mailFromName').text('Please Enter Valid Name!');

				$('#error_mailFromName').fadeIn(400).show();

				$("#mailFromName").focus();

				return false;

			}

			if(companyname == '')

			{

				$('#error_mailCompanyName').text('Please Enter Company Name!');

				$('#error_mailCompanyName').fadeIn(400).show();

				$("#mailCompanyName").focus();

				return false;

			}

			if( companyname.indexOf('www.') >= 0){ 

  				$('#error_mailCompanyName').text('Please Enter Valid Company Name!');

				$('#error_mailCompanyName').fadeIn(400).show();

				$("#mailCompanyName").focus();

				return false;

			}

			

			if( companyname.indexOf('http://') >= 0){

			  	$('#error_mailCompanyName').text('Please Enter Valid Company Name!');

				$('#error_mailCompanyName').fadeIn(400).show();

				$("#mailCompanyName").focus();

				return false;

			}

				

			if(formFeedbackSubject == '')

			{

				$('#error_mailSubject').text('Please Enter Subject!');

				$('#error_mailSubject').fadeIn(400).show();

				$("#mailSubject").focus();

				return false;

			}

			if( formFeedbackSubject.indexOf('www.') >= 0){

  				$('#error_mailSubject').text('Please Enter Valid Subject Name!');

				$('#error_mailSubject').fadeIn(400).show();

				$("#mailSubject").focus();

				return false;

			}

			

			if( formFeedbackSubject.indexOf('http://') >= 0){

			  	$('#error_mailSubject').text('Please Enter Valid Subject Name!');

				$('#error_mailSubject').fadeIn(400).show();

				$("#mailSubject").focus();

				return false;

			}	

			if(formFeedbackEmail == '')

			{

				$('#error_mailFrom').text('Please Enter Email!');

				$('#error_mailFrom').fadeIn(400).show();

				$("#mailFrom").focus();

				return false;

			}	

			

			if(!emailReg.test(formFeedbackEmail))

			{

				$('#error_mailFrom').text('Please Enter Valid Email!');

				$('#error_mailFrom').fadeIn(400).show();

				$("#mailFrom").focus();

				return false;

			}			

			});

		

	});

</script>



<script language='JavaScript' type="text/javascript" >







var ck_email = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i 





function reportErrors(errors){

 var msg = "Please Enter The Following Valid Data...\n";

 for (var i = 0; i<errors.length; i++) {

  var numError = i + 1;

  msg += "\n" + numError + ". " + errors[i];

 }

 alert(msg);

}





function validateFormOnSubmit(frm)

{

var formFeedbackName = frm.mailFromName.value; 

var companyname = frm.mailCompanyName.value; 

var formFeedbackSubject = frm.mailSubject.value;

var formFeedbackEmail = frm.mailFrom.value;



var ck_email = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;

var errors = [];



 if (formFeedbackName=="") {

  errors[errors.length] = "Please Enter valid Name.";

 }

 

 if (companyname=="") {

  errors[errors.length] = "Please Enter valid Company Name.";

 }

 

 if (formFeedbackSubject=="") {

  errors[errors.length] = "Please Enter valid Subject.";

 } 

  if (formFeedbackEmail=="") {

  errors[errors.length] = "Please Enter Email.";

 }  

  if (!ck_email.test(formFeedbackEmail)) {

  errors[errors.length] = "Please Enter valid  Email.";

 }

 

  if (errors.length > 0) {

  reportErrors(errors);

  return false;

 }

 

 return true;

 

}





</script>





