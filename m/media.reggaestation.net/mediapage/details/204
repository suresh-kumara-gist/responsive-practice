<script language='JavaScript' type="text/javascript" >







var ck_email = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i 





function reportErrors(errors){

 var msg = "Please Enter The Following Valid Data...\n";

 for (var i = 0; i<errors.length; i++) {

  var numError = i + 1;

  msg += "\n" + numError + ". " + errors[i];

 }

 alert(msg);

}





function validateFormOnSubmit(frm)

{

var formFeedbackName = frm.mailFromName.value; 

var companyname = frm.mailCompanyName.value; 

var formFeedbackSubject = frm.mailSubject.value;

var formFeedbackEmail = frm.mailFrom.value;



var ck_email = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;

var errors = [];



 if (formFeedbackName=="") {

  errors[errors.length] = "Please Enter valid Name.";

 }

 

 if (companyname=="") {

  errors[errors.length] = "Please Enter valid Company Name.";

 }

 

 if (formFeedbackSubject=="") {

  errors[errors.length] = "Please Enter valid Subject.";

 } 

  if (formFeedbackEmail=="") {

  errors[errors.length] = "Please Enter Email.";

 }  

  if (!ck_email.test(formFeedbackEmail)) {

  errors[errors.length] = "Please Enter valid  Email.";

 }

 

  if (errors.length > 0) {

  reportErrors(errors);

  return false;

 }

 

 return true;

 

}





</script>





